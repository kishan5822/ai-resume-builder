{
  "name": "ai-resume-builder",
  "version": "1.0.0",
  "description": "AI-powered resume builder with real-time LaTeX editing and 380+ AI models",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "server": "cd server && npm run dev",
    "build": "cd client && npm run build",
    "start": "npm run dev"
  },
  "keywords": [
    "resume-builder",
    "latex",
    "ai",
    "openrouter",
    "gemini",
    "react",
    "nodejs",
    "pdf-generator",
    "career",
    "job-search"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-resume-builder.git"
  },
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
